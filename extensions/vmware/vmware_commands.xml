<categories>
	<category name="vsphere" label="vSphere" description="Commands for interacting VMware vCenter and vSphere hosts." icon="static/images/icons/vmware/vmware.png">
		<commands>
			<command name="vmw_list_images" label="VMware List Images" description="" icon="static/images/icons/vmware/vmware.png">
				<function name="vmw_list_images" extension="vmware.vmware" variables="true" parse_method="1">
					<endpoint_name input_type="dropdown" label="vCenter Endpoint" hr_after="true" class="combo w300px" datasource="function" dataset="ddDataSource_GetVMwareClouds" />
					<instance_uuid label="InstanceUUID" input_type="text" />
					<filters label="Filters" is_array="true">
						<filter label="Filter">
							<name label="Name" input_type="text" />
							<values label="Values" is_array="true">
								<value label="Value" input_type="text" />
							</values>
						</filter>
					</filters>
				</function>
			</command>
			<command name="vmw_power_on_image" label="VMware Power On Image" description="" icon="static/images/icons/vmware/poweron.png">
				<function name="vmw_power_on_image" extension="vmware.vmware" variables="true" parse_method="1">
					<endpoint_name input_type="dropdown" label="vCenter Endpoint" hr_after="true" class="combo w300px" datasource="function" dataset="ddDataSource_GetVMwareClouds" />
					<instance_uuid label="InstanceUUID" input_type="text" />
				</function>
			</command>
			<command name="vmw_power_off_image" label="VMware Power Off Image" description="" icon="static/images/icons/vmware/poweroff.png">
				<function name="vmw_power_off_image" extension="vmware.vmware" variables="true" parse_method="1">
					<endpoint_name input_type="dropdown" label="vCenter Endpoint" hr_after="true" class="combo w300px" datasource="function" dataset="ddDataSource_GetVMwareClouds" />
					<instance_uuid label="InstanceUUID" input_type="text" />
				</function>
			</command>
			<command name="vmw_clone_image" label="VMware Clone Image" description="" icon="static/images/icons/vmware/vmware.png">
				<function name="vmw_clone_image" extension="vmware.vmware" variables="true" parse_method="1">
					<endpoint_name input_type="dropdown" label="vCenter Endpoint" hr_after="true" class="combo w300px" datasource="function" dataset="ddDataSource_GetVMwareClouds" />
					<instance_uuid label="InstanceUUID" input_type="text" />
					<name label="New Name" input_type="text" />
					<folder label="Folder" input_type="text" />
					<resourcepool label="Resource Pool" input_type="text" />
					<power_on label="Power On (yes/no)" input_type="text" />
				</function>
			</command>
		</commands>
	</category>
	</categories>
